{
    "name": "laxmidhar/laravel-helpers",
    "description": "A handy little toolkit that makes Laravel do the boring stuff so you don't have to.",
    "type": "library",
    "license": "MIT",
    "version": "1.2.0",
    "authors": [
        {
            "name": "Laxmidhar Maharana",
            "email": "papu.team@example.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "nesbot/carbon": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Laxmidhar\\LaravelHelpers\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laxmidhar\\LaravelHelpers\\HelperServiceProvider"
            ],
            "aliases": {
                "Helper": "Laxmidhar\\LaravelHelpers\\Facades\\Helper"
            }
        }
    },
    "keywords": [
        "laravel",
        "helpers",
        "toolkit",
        "utilities"
    ],
    "homepage": "https://github.com/laxmidhar/laravel-helpers",
    "support": {
        "issues": "https://github.com/laxmidhar/laravel-helpers/issues",
        "source": "https://github.com/laxmidhar/laravel-helpers"
    }
}